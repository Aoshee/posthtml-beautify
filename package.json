{
  "name": "posthtml-beautify",
  "version": "0.0.0",
  "description": "Beautifier for html",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "xo src/**/*.js test/**/*.js && nyc ava",
    "clean": "rm -rf lib && mkdir lib",
    "build": "npm run clean && babel src/ -d lib/",
    "prepublish": "npm run build",
    "update": "updtr && ava-codemods --force"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GitScrum/posthtml-beautify.git"
  },
  "keywords": [
    "html",
    "posthtml",
    "posthtml-plugin",
    "beautify",
    "beautifier",
    "code-quality"
  ],
  "dependencies": {
    "posthtml-parser": "^0.1.3",
    "posthtml-render": "^1.0.6"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "ava-codemods": "^0.2.1",
    "babel-cli": "^6.10.1",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-min": "^0.1.5",
    "babel-register": "^6.7.2",
    "coveralls": "^2.11.9",
    "is-promise": "^2.1.0",
    "nyc": "^6.4.4",
    "posthtml": "^0.8.7",
    "updtr": "^0.2.0",
    "xo": "^0.16.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ],
    "comments": false,
    "compact": true,
    "minified": true
  },
  "author": {
    "name": "GitScrum",
    "email": "Scrum@list.ru",
    "url": "https://twitter.com/Scrum_"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GitScrum/posthtml-beautify/issues"
  },
  "homepage": "https://github.com/GitScrum/posthtml-beautify#readme"
}
